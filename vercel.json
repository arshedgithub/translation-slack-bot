{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.13" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
      "api/index.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }